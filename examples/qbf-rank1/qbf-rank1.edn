  :2-thm5-2
  {:id :2-thm5-2
   :type :claim
   :statement "Summing over all $\\alpha \\neq 0$: $\\sum_{\\alpha \\neq 0} p_\\alpha(2n-2) = (2n-2)(1 - p_0)$"
   :dependencies #{:2-thm5-1}
   :scope #{}
   :justification :algebraic-rewrite
